<?xml version="1.0" encoding="utf-8"?>
<!--Twillio video call plugin's resources in local-->
<!--Added by Rupesh for videocall library with private repo-->
<resources>
    <attr format="boolean" name="tviMirror"/>
    <attr format="boolean" name="tviOverlaySurface"/>
    <attr format="enum" name="tviScaleType">
        <enum name="fit" value="0"/>
        <enum name="fill" value="1"/>
        <enum name="balanced" value="2"/>
    </attr>
    <declare-styleable name="VideoTextureView">
        <attr name="tviMirror"/>
        <attr name="tviScaleType"/>
    </declare-styleable>
    <declare-styleable name="VideoView">
        <attr name="tviMirror"/>
        <attr name="tviScaleType"/>
        <attr name="tviOverlaySurface"/>
    </declare-styleable>
</resources>